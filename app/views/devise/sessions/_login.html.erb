<div class="container">
	<div class="row">
    
  </div>
		
        <div class="modal fade" id="loginModal">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header">
              <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
              <h4 class="modal-title">Login to happyshot.com</h4>
            </div>
          <div class="modal-body">

            <%= bootstrap_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
              <p>
                <%= f.email_field :email, autofocus: true, class: 'span3', placeholder: 'Email' %>
              </p>
              <p>
                 <%= f.password_field :password, autocomplete: "off", class: "span3", placeholder: "Password" %>
                 
  
              </p>
              <p>
                 <%= f.submit "Sign in", class: "btn btn-primary" %>
                 <a href="#forgetPasswdModal" data-toggle="modal" data-dismiss="modal" class ="btn btn-link">
              Forgot Password?
              </a>
              <br />
              </p>
            <% end %>
          </div>
          <div class="modal-footer">
            New To happyshot.com?
            
             <a href="#registerModal" data-toggle="modal" data-dismiss="modal" class ="btn btn-primary">
              Register
              </a>
          </div>
        </div>
	</div>
</div>