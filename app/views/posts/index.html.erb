     <script class="gifshot-code-template" type="text/template">
     gifshot.createGIF({
    gifWidth: 230,
    gifHeight: 230,
    video: ['/cache/1bdb72f8b36f61cc51d814fb83559a68b128a6235a1799ca45785fdcf6aa'],
    interval: 1,
    text: 'happyShot',
    fontSize: '18px',
    fontFamily: 'Waltograph',
    textAlign: 'left'
}, function (obj) {
    if (!obj.error) {
        var image = obj.image, animatedImage = document.createElement('img');
        animatedImage.src = image;
        document.body.appendChild(animatedImage);
    }
});
     </script>
     
    <script>
      $(document).ready(function() {
    var text_max = 130;
    $('#textarea_feedback').html(text_max + ' characters remaining');

    $('#gifData').keyup(function() {
        var text_length = $('#gifData').val().length;
        var text_remaining = text_max - text_length;

        $('#textarea_feedback').html(text_remaining + ' characters remaining');
    });
});
      </script>

      <script>
      function downloadURI(uri, name) {
        var link = document.createElement("a");
        link.download = name;
        link.href = uri;
        link.click();
      }
      </script>

